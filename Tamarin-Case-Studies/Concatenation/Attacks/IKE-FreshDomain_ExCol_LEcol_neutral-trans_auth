   [ B0( pkA, ~skB ), In( <'INIT', SA_A, gX, nA, infoA> ), Fr( ~cookie ) ]
  --[ Restr_B_0_1( pk(~skB), pkA ) ]->
   [
   B1( pkA, ~skB, ~cookie, <SA_A, gX, nA, infoA> ), Out( <'INIT', ~cookie> )
   ]

  /* has exactly the trivial AC variant */

